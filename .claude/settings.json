{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "hooks": {
    "Notification": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "$HOME/bin/claude_notify"
          }
        ]
      }
    ],
    "PreCompact": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "bd prime"
          }
        ]
      }
    ],
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "$HOME/bin/claude_notify"
          }
        ]
      },
      {
        "hooks": [
          {
            "type": "command",
            "command": "node \"/Users/justin/.claude/hooks/prove-it-gate.js\"",
            "timeout": 3600
          }
        ]
      }
    ],
    "SessionStart": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "bd prime"
          }
        ]
      },
      {
        "matcher": "startup|resume|clear|compact",
        "hooks": [
          {
            "type": "command",
            "command": "node \"/Users/justin/.claude/hooks/prove-it-session-start.js\""
          }
        ]
      }
    ],
    "PreToolUse": [
      {
        "matcher": "Bash",
        "hooks": [
          {
            "type": "command",
            "command": "node \"/Users/justin/.claude/hooks/prove-it-gate.js\""
          }
        ]
      },
      {
        "matcher": "Edit|Write|NotebookEdit|Bash",
        "hooks": [
          {
            "type": "command",
            "command": "node \"/Users/justin/.claude/hooks/prove-it-beads-gate.js\""
          }
        ]
      }
    ]
  },
  "enabledPlugins": {
    "beads@beads-marketplace": true,
    "swift-lsp@claude-plugins-official": true
  },
  "autoUpdatesChannel": "latest",
  "feedbackSurveyState": {
    "lastShownTime": 1754063063998
  }
}
